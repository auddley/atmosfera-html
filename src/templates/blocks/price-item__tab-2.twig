{% set priceList1 = [
    {
        value: '96',
        price: '6,0',
        time: '1-2 дней',
        href: '#',
    },
    {
        value: '192',
        price: '5,0',
        time: '1-2 дней',
        href: '#',
    },
] %}

{% set priceList2 = [
    {
        value: '196',
        price: '10,0',
        time: '4-5 дней',
        href: '#',
    },
    {
        value: '292',
        price: '15,0',
        time: '4-5 дней',
        href: '#',
